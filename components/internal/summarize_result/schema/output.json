{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["dependencyGraph", "rootIntegrity", "warnings"],
  "properties": {
    "dependencyGraph": {
      "type": "array",
      "description": "Resolved dependency graph for the root component."
    },
    "rootIntegrity": {
      "type": ["string", "null"],
      "description": "Integrity hash of the root descriptor, when available."
    },
    "warnings": {
      "type": "array",
      "items": { "type": "string" },
      "description": "Aggregated warnings."
    }
  },
  "additionalProperties": true
}
